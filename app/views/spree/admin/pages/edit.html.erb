<%= render partial: 'spree/admin/shared/page_tabs', locals: {page: @object, current: 'Page Details'} %>

<%= form_for([:admin, @page]) do |form| %>
  <%= render 'spree/admin/shared/error_messages', target: @page %>

  <fieldset data-hook="edit_page">
    <%= render "form", form: form %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>