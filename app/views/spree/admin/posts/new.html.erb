<% content_for :page_title do %>
  <%= link_to Spree.t('admin.subnav.posts'), admin_posts_path %> / <%= Spree.t("essential_content.admin.post_new") %>
<% end %>

<%= render 'spree/admin/shared/error_messages', target: @post %>

<%= form_for([:admin, @post]) do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>